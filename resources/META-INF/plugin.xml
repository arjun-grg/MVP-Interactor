<idea-plugin>
    <id>com.wongel.MVPGenerator</id>
    <name>MVP-Interactor Generator</name>
    <version>1.0</version>
    <vendor email="tsering.wongel@gmail.com" url="">Tsering Wongel</vendor>

    <description><![CDATA[
    Mvp Interactor Generator:
      This plugin is used to create MVP package with Presenter,view and interactor both in java/kotlin.<br>
      This plugin uses Mosby "version-mosby3" as default MVP handler.<br>
    ]]></description>

    <change-notes><![CDATA[
    ]]>
    </change-notes>

    <idea-version since-build="145.0"/>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <action id="MvpPackage.id" class="com.wongel.MVPGenerator.MvpPackage"
                text="New Mvp Package" description="New Mvp Package" >
            <add-to-group group-id="NewGroup" anchor="first"/>
        </action>
    </actions>

</idea-plugin>